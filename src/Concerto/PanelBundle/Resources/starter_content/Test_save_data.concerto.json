        "code": "db = fromJSON(data_bank)\n\ntable = \"default_data_table\"\nsession_internal_id_column = \"session_internal_id\"\nname_column = \"name\"\nvalue_column = \"value\"\nif(db$table_type == \"custom\") {\n  table = db$custom_table$table\n  session_internal_id_column = db$custom_table$columns$session_internal_id\n  name_column = db$custom_table$columns$name\n  value_column = db$custom_table$columns$value\n}\n\nsession_internal_id = 0\nif(!is.null(session) && is.list(session)) {\n  session_internal_id = session$internal_id\n} else {\n  session_internal_id = concerto$session$id\n}\n\nfor(name in ls(new_data)) {\n  record = concerto.table.query(\"SELECT * FROM {{table}} WHERE {{session_internal_id_column}} = {{session_internal_id}} AND {{name_column}} = '{{name}}'\", list(\n    table=table, \n    session_internal_id_column=session_internal_id_column, \n    session_internal_id=session_internal_id, \n    name_column=name_column, \n    name=name\n  ))\n  if(dim(record)[1] > 0) { \n    concerto.table.query(\"UPDATE {{table}} SET {{value_column}} = '{{value}}' WHERE {{session_internal_id_column}} = {{session_internal_id}} AND {{name_column}} = '{{name}}'\", list(\n      table=table, \n      value_column=value_column, \n      value=new_data[[name]], \n      session_internal_id_column=session_internal_id_column, \n      session_internal_id=session_internal_id, \n      name_column=name_column, \n      name=name\n    ))\n  } else {\n    concerto.table.query(\"INSERT INTO {{table}} SET {{value_column}} = '{{value}}', {{session_internal_id_column}} = {{session_internal_id}}, {{name_column}} = '{{name}}'\", list(\n      table=table, \n      value_column=value_column, \n      value=new_data[[name]], \n      session_internal_id_column=session_internal_id_column, \n      session_internal_id=session_internal_id, \n      name_column=name_column, \n      name=name\n    ))\n  }\n}",
